{
  "name": "nenkraft",
  "description": "A small rendering tool",
  "author": "nuuf",
  "version": "1.1.6",
  "main": "index.js",
  "license": "MIT",
  "eslintConfig": "./.eslintrc",
  "devDependencies": {
    "autoprefixer": "*",
    "colors": "*",
    "cross-env": "*",
    "css-loader": "*",
    "copy-webpack-plugin": "*",
    "eslint": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "*",
    "html-webpack-plugin": "*",
    "postcss-loader": "*",
    "raw-loader": "*",
    "rimraf": "*",
    "style-loader": "*",
    "url-loader": "*",
    "webpack": "3.*",
    "webpack-cli": "*",
    "webpack-dev-server": "2.*"
  },
  "scripts": {
    "s-d": "cross-env NODE_ENV=development",
    "s-p": "cross-env NODE_ENV=production",
    "s-l": "cross-env NODE_ENV=librarybundle",
    "clean": "rimraf dist",
    "clean-package": "rimraf ./package/dist",
    "clean-everything": "npm run clean && npm run clean-package",
    "lint": "eslint src",
    "lint-f": "eslint src --fix",
    "lint-tests": "eslint tests",
    "lint-tests-f": "eslint tests --fix",
    "start": "npm run s-d node deve-server.js",
    "build-dev": "npm run clean && npm run s-d webpack",
    "build": "npm run clean && npm run s-p webpack",
    "build-full": "npm run build && npm run s-p webpack -- --env.bigBundle",
    "build-package": "npm run clean-package && npm run s-l webpack -- --env.libBundle",
    "build-everything": "npm run build-full && npm run build-package",
    "electron": "electron ./electron/main.js",
    "ce": "npm run clean-everything",
    "be": "npm run build-everything"
  }
}
